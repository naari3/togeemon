{
    "manifest_version": 2,
    "name": "とげえもん",
    "description": "助けてとげえもん",
    "version": "1.1.1",
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/contentScripts.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": []
}